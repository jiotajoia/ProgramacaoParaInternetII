<h2>Montadoras Cadastradas</h2>

<table>
    <tr>
        <th>ID</th>
        <th> Nome </th>
        <th> País </th>
        <th> Ano de Fundação </th>
    </tr>
    {{#each montadoras}}
        <tr>
            <td>{{id_montadora}}</td>
            <td>{{nome}}</td>
            <td>{{pais}}</td>
            <td>{{ano_fundacao}}</td>
            <td><a href='/deletarMontadora/{{this.id_montadora}}'>Deletar Montadora</a></td>
            <td><a href="/editarMontadora/{{this.id_montadora}}">Editar Montadora</a></td>
        </tr>
    {{/each}}
</table>
    
